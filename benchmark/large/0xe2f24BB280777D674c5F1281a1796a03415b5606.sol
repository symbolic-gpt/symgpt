    string public name;

    // Token registry symbol
    string public symbol;

    struct Stake {
        uint256 amount;
        uint256 startTime;
        uint256 duration;
        string referralCode;
    }

    mapping(address => Stake[]) public stakes;
    mapping(uint256 => uint256) public apys;
    mapping(string => address) public referralOwners;
    mapping(address => string) public hasReferral;

    // Metadata URIs per token ID
    mapping(uint256 => string) public tokenURI;
    
    event Staked(address indexed user, uint256 amount, uint256 duration, string referralCode);
    event Withdrawn(address indexed user, uint256 amount, uint256 reward);
    event EarlyWithdrawn(address indexed user, uint256 amount, uint256 penalty);
    event ReferralRegistered(address indexed user, string referralCode);
    event NFTMinted(address indexed user, uint256 tokenId, uint256 amount);

