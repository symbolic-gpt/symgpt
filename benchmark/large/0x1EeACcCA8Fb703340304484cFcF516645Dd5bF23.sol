    }

    /**
     * @dev Constructor of the contract with initial supply, dev wallet, reflect NFT address, vault address, and pre-sale contract address.
     * @param initialSupply Initial token supply.
     * @param _devWallet Address of the dev wallet.
     * @param _vaultAddress Address of the vault.
     */
    constructor(uint256 initialSupply, address _devWallet, address _vaultAddress, address _initialOwner)
        ERC20("Golden Goose", "GooS")
        Ownable(_initialOwner)
    {
        require(initialSupply <= MAX_SUPPLY, "Initial supply exceeds max supply");
        _mint(msg.sender, initialSupply);
        
        // Update max transaction amount to 0.12% of the current supply
        maxTransactionAmount = initialSupply * 12 / 10000;

        // Initialize other variables
        devWallet = _devWallet;
        vaultAddress = _vaultAddress;

        isExcludedFromFee[msg.sender] = true;
        isExcludedFromMaxTransaction[msg.sender] = true;
    }

    /**
     * @dev Internal transfer function with additional functionality to handle fees.
     * @param sender Address of the sender.
     * @param recipient Address of the recipient.
     * @param amount Amount of tokens to transfer.
     */
    function _transfer(address sender, address recipient, uint256 amount) internal virtual override(ERC20) {
        require(amount < maxTransactionAmount, "ERC20: Cannot transfer more than maximum transfer limit");
        uint256 totalFees;

        if (!isExcludedFromFee[sender]) {
            uint256 devAmount = (amount * devPercentage) / 100; // 1% to dev wallet
            uint256 reflectAmount = (amount * reflectPercentage) / 100; // 1% to Reflect NFT holders
            uint256 vaultAmount = (amount * vaultPercentage) / 100; // 1% to vault

            totalFees = devAmount + reflectAmount + vaultAmount;
            amount -= totalFees;

            address pair = IUniswapV2Factory(uniswapV2Router.factory()).getPair(address(this), uniswapV2Router.WETH());
            bool isLiquidityAvailable = pair != address(0);

            if(isLiquidityAvailable) {
                (uint112 reserve0, uint112 reserve1, ) = IUniswapV2Pair(pair).getReserves();
                // Ensure there is liquidity in the pair
                isLiquidityAvailable = reserve0 > 0 && reserve1 > 0;

                if(isLiquidityAvailable) {
                    // Swap tokens for ETH here
                    uint256 initialBalance = address(this).balance;
                    _swapTokensForEth(devAmount + vaultAmount);
                    uint256 transferredBalance = address(this).balance - initialBalance;

                    // Calculate the portion of the swapped ETH to send to the devWallet and vaultAddress
                    uint256 devETHAmount = transferredBalance * devAmount / (devAmount + vaultAmount);
                    uint256 vaultETHAmount = transferredBalance - devETHAmount;

                    // Transfer ETH to devWallet and vaultAddress
                    payable(devWallet).transfer(devETHAmount);
                    payable(vaultAddress).transfer(vaultETHAmount);
                }
            } else {
                // Transfer fees as tokens if no liquidity
                super._transfer(sender, reflectNFTAddress, reflectAmount);
                super._transfer(sender, devWallet, devAmount);
                super._transfer(sender, vaultAddress, vaultAmount);
            }
        }

        super._transfer(sender, recipient, amount);
    }

    /**
     * @dev Private function to swap tokens to native currency i.e. MATIC.
     * @param tokenAmount The amount of tokens to be swapped.
     */
    function _swapTokensForEth(uint256 tokenAmount) private {
        // Generate the uniswap pair path of token -> WETH
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // Make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this), // The contract itself
            block.timestamp
        );
    }

