struct AssetRoyalty {
    uint256 assetId;
    address receiver;
    uint96 percentage;
}

abstract contract RoyaltyManager is ERC2981Upgradeable, AccessControlUpgradeableEx {
    uint96 private constant PERCENTAGE_100 = 100_000;

    event SetDefaultRoyalty(address indexed operator, address indexed receiver, uint96 percentage);
    event SetTokenRoyalties(address indexed operator, AssetRoyalty[] royalties);

    function _initializeRoyaltyManager(
        address _defaultRoyaltyReceiver,
        uint96 _defaultRoyaltyPercentage
    ) internal {
        if (_defaultRoyaltyReceiver != address(0)) {
            _setDefaultRoyalty(_defaultRoyaltyReceiver, _defaultRoyaltyPercentage);
        } else {
            require(_defaultRoyaltyPercentage == 0, "RoyaltyManager-1: invalid default royalty");
        }
        emit SetDefaultRoyalty(address(0), _defaultRoyaltyReceiver, _defaultRoyaltyPercentage);
    }

    modifier onlyRoyaltyAdmin() {
        require(isRoyaltyAdmin(_msgSender()), "RoyaltyManager-2: not royalty admin");
        _;
    }

    function isRoyaltyAdmin(address _account) public view virtual returns (bool);

    function _feeDenominator() internal pure virtual override returns (uint96) {
        return PERCENTAGE_100;
    }

    function setDefaultRoyalty(address _receiver, uint96 _percentage) external onlyRoyaltyAdmin {
        _setDefaultRoyalty(_receiver, _percentage);
        emit SetDefaultRoyalty(_msgSender(), _receiver, _percentage);
    }

    function deleteDefaultRoyalty() external onlyRoyaltyAdmin {
        _deleteDefaultRoyalty();
        emit SetDefaultRoyalty(_msgSender(), address(0), 0);
    }

    function setTokenRoyalties(AssetRoyalty[] calldata _royalties) external onlyRoyaltyAdmin {
        require(_royalties.length != 0, "RoyaltyManager-3: empty data");

        for (uint256 i; i < _royalties.length; ) {
            if (_royalties[i].receiver == address(0)) {
                require(_royalties[i].percentage == 0, "RoyaltyManager-4: invalid token royalty");
                _resetTokenRoyalty(_royalties[i].assetId);
            } else {
                require(_royalties[i].percentage != 0, "RoyaltyManager-5: invalid token royalty");
                _setTokenRoyalty(_royalties[i].assetId, _royalties[i].receiver, _royalties[i].percentage);
            }

             unchecked { ++i; }
        }

        emit SetTokenRoyalties(_msgSender(), _royalties);
    }

    function supportsInterface(
        bytes4 _interfaceId
    ) public view virtual override(
        ERC2981Upgradeable, AccessControlUpgradeable
    ) returns (bool) {
        return ERC2981Upgradeable.supportsInterface(_interfaceId)
            || AccessControlUpgradeable.supportsInterface(_interfaceId);
    }
}

enum NftType{
    MysticFlameShard,
    MysticWaveShard,
    MysticWindShard,
    MysticEarthShard,

    ArcaneFlameShard,
    ArcaneWaveShard,
    ArcaneWindShard,
    ArcaneEarthShard,

    DivineFlameShard,
    DivineWaveShard,
    DivineWindShard,
    DivineEarthShard,

    CelestialFlameShard,
    CelestialWaveShard,
    CelestialWindShard,
    CelestialEarthShard,

    MysticElementsChest,
    ArcaneSecretsCrate,
    DivineCosmosVault,
    CelestialHarmonyStrongbox
}

struct MintItem {
    address recipient;
    NftType[] nftTypes;
    uint256[] amounts;
    bytes data;
}

contract BountyTempleNft is ERC1155SupplyUpgradeable, RoyaltyManager {
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    function version() external pure returns (string memory) { return "BountyTempleNft v1"; }

    constructor() {
        _disableInitializers();
    }

    function initialize(
        string memory _uri,
        address _admin,
        address[] calldata _minters,
        address _defaultRoyaltyReceiver,
        uint96 _defaultRoyaltyPercentage
    ) external initializer {
        __ERC1155_init(_uri);

        _grantRole(DEFAULT_ADMIN_ROLE, _admin);
        _grantRole(MINTER_ROLE, _admin);

        _grantRoleBatch(MINTER_ROLE, _minters);

        _initializeRoyaltyManager(_defaultRoyaltyReceiver, _defaultRoyaltyPercentage);
    }

    function setUri(string memory _uri) external onlyRole(DEFAULT_ADMIN_ROLE) {
        _setURI(_uri);
    }

    function isRoyaltyAdmin(address _account) public view override returns (bool) {
        return hasRole(DEFAULT_ADMIN_ROLE, _account);
    }

    function mint(MintItem[] calldata _mintItems) external onlyRole(MINTER_ROLE) {
        require(_mintItems.length != 0, "BTN-1: empty data");

        for (uint256 i; i < _mintItems.length; ) {
            _mintTo(_mintItems[i].recipient, _mintItems[i].nftTypes, _mintItems[i].amounts, _mintItems[i].data);
            unchecked { ++i; }
        }
    }

    function craft(NftType _nftType, uint256 _amount) external {
        require(_amount != 0, "BTN-2: zero craft amount");

        address operator = _msgSender();

        if (_nftType == NftType.MysticElementsChest) {
            _burn(operator, uint256(NftType.MysticFlameShard), _amount);
            _burn(operator, uint256(NftType.MysticWaveShard), _amount);
            _burn(operator, uint256(NftType.MysticWindShard), _amount);
            _burn(operator, uint256(NftType.MysticEarthShard), _amount);
            _mint(operator, uint256(NftType.MysticElementsChest), _amount, "");
        } else if (_nftType == NftType.ArcaneSecretsCrate) {
            _burn(operator, uint256(NftType.ArcaneFlameShard), _amount);
            _burn(operator, uint256(NftType.ArcaneWaveShard), _amount);
            _burn(operator, uint256(NftType.ArcaneWindShard), _amount);
            _burn(operator, uint256(NftType.ArcaneEarthShard), _amount);
            _mint(operator, uint256(NftType.ArcaneSecretsCrate), _amount, "");
        } else if (_nftType == NftType.DivineCosmosVault) {
            _burn(operator, uint256(NftType.DivineFlameShard), _amount);
            _burn(operator, uint256(NftType.DivineWaveShard), _amount);
            _burn(operator, uint256(NftType.DivineWindShard), _amount);
            _burn(operator, uint256(NftType.DivineEarthShard), _amount);
            _mint(operator, uint256(NftType.DivineCosmosVault), _amount, "");
        } else if (_nftType == NftType.CelestialHarmonyStrongbox) {
            _burn(operator, uint256(NftType.CelestialFlameShard), _amount);
            _burn(operator, uint256(NftType.CelestialWaveShard), _amount);
            _burn(operator, uint256(NftType.CelestialWindShard), _amount);
            _burn(operator, uint256(NftType.CelestialEarthShard), _amount);
            _mint(operator, uint256(NftType.CelestialHarmonyStrongbox), _amount, "");
        } else {
            revert("BTN-3: invalid craft type");
        }
    }

    function _mintTo(
        address _recipient,
        NftType[] calldata _nftTypes,
        uint256[] calldata _amounts,
        bytes calldata _data
    ) private {
        if (_nftTypes.length == 1) {
            _mint(_recipient, uint256(_nftTypes[0]), _amounts[0], _data);
        } else {
            _mintBatch(_recipient, _toUint256Array(_nftTypes), _amounts, _data);
        }
    }

    function _toUint256Array(NftType[] calldata _nftTypes) private pure returns (uint256[] memory) {
        uint256[] memory result = new uint256[](_nftTypes.length);
        for (uint256 i; i < _nftTypes.length; ) {
            result[i] = uint256(_nftTypes[i]);
            unchecked { ++i; }
        }
        return result;
    }

    function supportsInterface(
        bytes4 _interfaceId
    ) public view virtual override(
        ERC1155Upgradeable, RoyaltyManager
    ) returns (bool) {
        return ERC1155Upgradeable.supportsInterface(_interfaceId)
            || RoyaltyManager.supportsInterface(_interfaceId);
    }
}