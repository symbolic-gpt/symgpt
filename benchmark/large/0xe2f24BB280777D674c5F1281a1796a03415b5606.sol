        referralOwners[_referralCode] = msg.sender;
        hasReferral[msg.sender] = _referralCode;

        emit ReferralRegistered(msg.sender, _referralCode);
    }

    function stake(uint256 _amount, uint256 _months, string memory _referralCode) external nonReentrant {
        require(apys[_months] > 0, "Invalid staking duration");
        require(referralOwners[_referralCode] != msg.sender,"Can't use your own referral");
        stakingToken.transferFrom(msg.sender, address(this), _amount);

        stakes[msg.sender].push(Stake({
            amount: _amount,
            startTime: block.timestamp,
            duration: _months * 30 days,
            referralCode: _referralCode
        }));

        emit Staked(msg.sender, _amount, _months, _referralCode);

        // Mint NFT if within first 100 id1 and 900 id2 stakes
        if (nextTokenId1 <= 100 && _amount >= minAmountForNFT ) {
            _mint(msg.sender, 1, 1, "");
            nextTokenId1++;
            emit NFTMinted(msg.sender, 1, 1);
        } else if (nextTokenId2 <= 900 && _amount >= minAmountForNFT ) {
            _mint(msg.sender, 2, 1, "");
            nextTokenId2++;
            emit NFTMinted(msg.sender, 2, 1);
        }

        totalStakedAmount+=_amount;
    }

    function withdraw(uint256 _stakeIndex) external nonReentrant {
        require(_stakeIndex < stakes[msg.sender].length, "Invalid stake index");

        Stake memory stakeData = stakes[msg.sender][_stakeIndex];
        uint256 endTime = stakeData.startTime + stakeData.duration;

        if (block.timestamp >= endTime) {
            uint256 reward = calculateReward(stakeData.amount, stakeData.duration);
            uint256 referralBonusUser = 0;
            uint256 referralBonusOwner = 0;
            uint256 totalAmount = stakeData.amount + reward;

            // Apply referral bonus if applicable
            if (bytes(stakeData.referralCode).length > 0) {
                address referralOwner = referralOwners[stakeData.referralCode];
                if (referralOwner != address(0) && referralOwner != msg.sender) {
                    referralBonusUser = (stakeData.amount * 3) / 100;
                    referralBonusOwner = (stakeData.amount * 1) / 100;
                    
                    totalAmount += referralBonusUser;
                    stakingToken.transfer(referralOwner, referralBonusOwner);
                }
            }

            // Apply NFT bonus if applicable
            uint256 nftBonus = getNFTBonusAndBurn(msg.sender, stakeData.amount);
            totalAmount += nftBonus;

            stakingToken.transfer(msg.sender, totalAmount);
            emit Withdrawn(msg.sender, stakeData.amount, stakeData.amount + referralBonusUser + nftBonus );
        } else {
            uint256 penalty = (stakeData.amount * 10) / 100;
            uint256 remainingAmount = stakeData.amount - penalty;
            stakingToken.transfer(msg.sender, remainingAmount);
            stakingToken.transfer(penaltyReceiver, penalty);
            emit EarlyWithdrawn(msg.sender, stakeData.amount, penalty);
        }
        totalStakedAmount = totalStakedAmount - stakeData.amount;
        removeStake(msg.sender, _stakeIndex);
        
    }

    function calculateReward(uint256 _amount, uint256 _duration) public view returns (uint256) {
        uint256 months = _duration / 30 days;
        uint256 apy = apys[months];
        return (_amount * apy * months) / (12 * 1000);
    }

    function getNFTBonusAndBurn(address _user, uint256 _amount) internal returns (uint256) {
        uint256 nftBonus = 0;
        if (balanceOf(_user, 1) > 0) {
            nftBonus = (_amount * 10) / 100;
            burn(_user, 1, 1);
        } else if (balanceOf(_user, 2) > 0) {
            nftBonus = (_amount * 3) / 100;
            burn(_user, 2, 1);
        }
        return nftBonus;
    }

    function removeStake(address _user, uint256 _index) internal {
        uint256 lastIndex = stakes[_user].length - 1;

        if (_index != lastIndex) {
            stakes[_user][_index] = stakes[_user][lastIndex];
        }

        stakes[_user].pop();
    }

    function withdrawAdmin(uint256 _amount) external onlyOwner {
        uint256 availableBalance = stakingToken.balanceOf(address(this)) - totalStakedAmount;
        require(_amount <= availableBalance, "Insufficient un-staked balance in contract");
        stakingToken.transfer(msg.sender, _amount);
    }

    function getStakes(address _user) external view returns (Stake[] memory) {
        return stakes[_user];
    }

    function setMinAmountForNFT(uint256 amount ) external onlyOwner {
        minAmountForNFT = amount;
    }


    /**
     * Sets metadata URI for a given token ID.
     *
     *
     * @param _id token ID
     * @param _uri metadata URI
     */
    function setURI(uint256 _id, string memory _uri) external onlyOwner {
       
        tokenURI[_id] = _uri;
        emit URI(_uri, _id);
    }

    /**
     * Fetches metadata URI for a given token ID.
     *
     * @param _id token ID
     */
    function uri(uint256 _id) public view override returns (string memory) {
        return tokenURI[_id];
    }

}