        stakingToken = IERC20(_stakingToken);
        penaltyReceiver = _penaltyReceiver;
        apys[3] = 125;    // 12.5% for 3 months
        apys[6] = 250;    // 25% for 6 months
        apys[9] = 375;    // 37.5% for 9 months
        apys[12] = 500;   // 50% for 12 months
        name = nftName;
        symbol = nftSymbol;
    }

