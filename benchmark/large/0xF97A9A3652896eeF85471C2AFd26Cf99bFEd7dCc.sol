    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );

  function latestRoundData()
    external
    view
    returns (
      uint80 roundId,
      int256 answer,
      uint256 startedAt,
      uint256 updatedAt,
      uint80 answeredInRound
    );
}

/**
 * @title RacinoCars
 * @dev An NFT collection leveraging ERC721A for efficient batch minting, ERC2981 for royalties, and supporting
 * payments in both Ether and ERC20 tokens. It incorporates features for public, private, and Crossmint-specific minting.
 */
contract RacinoCars is ERC721AEnumerable, Ownable, ERC2981 {
    using Strings for uint256;

    // Maximum number of NFTs that can be minted in this collection +1 to account for indexing starting at 1.
    uint256 public maxSalePlusOne = 10001;

    // Price of each NFT in USD, assuming 8 decimal places for precision (e.g., $5 is represented as 500000000).
    uint256 public tokenPriceUSD = 500000000 ether;
    // Price of each NFT in VEXT
    uint256 public tokenPriceVEXT = 5000000000000000000;

    // Limit on the number of NFTs that can be minted in a single transaction +1 to simplify checks.
    uint256 public txLimitPlusOne = 10;

    // Allowance of NFTs an address can mint +1 to simplify checks.
    uint256 public allowancePlusOne = 100;

    // Merkle root for validating private sale allowlists.
    // bytes32 public merkleRoot;

    address public USDC;
    address public VEXT;

    // Enumeration for the contract's operational states.
    enum ContractState {
        OFF, // Sales are turned off.
        PRIVATE, // Private sale phase, only allowlisted addresses can mint.
        PUBLIC // Public sale phase, open to all.
    }

    // The current operational state of the contract.
    ContractState public contractState = ContractState.PUBLIC;

    // Placeholder URI for unrevealed NFTs.
    string public placeholderURI;

    // Base URI for revealed NFT metadata.
    string public baseURI;

    // Chainlink price feed interface for fetching live ETH/USD exchange rates.
    AggregatorV3Interface public priceFeed;

    // Address that will hold all of the funds from sales
    address public treasury;

    mapping(address => bool) public admins;

    /**
     * @dev Constructor function that initializes the NFT collection, setting up the name, symbol, price feed, and default royalties.
     */
    constructor() ERC721AEnumerable("Racino Cars", "RC") {
        priceFeed = AggregatorV3Interface(0xd0D5e3DB44DE05E9F294BB0a3bEEaF030DE24Ada); // Initialize the price feed with the Chainlink ETH/USD address.

        _setRoyalties(owner(), 10000); // Set the default royalties to 10%.
        treasury = msg.sender;
    }

    ///
    /// MODIFIERS
    ///

    /**
     * @dev Ensures the function is called in the correct contract state.
     * @param contractState_ The required state for the function to proceed.
     */
    modifier isContractState(ContractState contractState_) {
        require(contractState == contractState_, "Invalid state");
        _;
    }

    /**
     * @dev Ensures the minting quantity does not exceed the max supply limit.
     * @param quantity The number of NFTs being minted in the transaction.
     */
    modifier withinMintLimit(uint256 quantity) {
        require((_totalMinted() + quantity) < maxSalePlusOne, "Exceeds available tokens");
        _;
    }

    /**
     * @dev Ensures the sent Ether value matches or exceeds the expected price for the minting operation.
     * @param expectedValue The expected Ether value required for the transaction to proceed.
     */
    modifier correctValue(uint256 expectedValue) {
        require(expectedValue <= msg.value, "Ether value sent is not correct");
        _;
    }

    /**
     * @dev Ensures only an admin can access the function
     */
    modifier onlyAdmin(address admin) {
        require(admins[admin], "Not an admin");
        _;
    }

    ///
    /// PRICE FEED
    ///

    /**
     * @dev Fetches the latest ETH to USD price from the Chainlink Price Feed.
     * @return The latest price as an integer.
     */
    function getTokentoUSD() public view returns(int) {
        (, int price,,,) = priceFeed.latestRoundData();
        return price;
    }

    /**
     * @dev Calculates the Ether price for one NFT based on the latest ETH/USD price.
     * @return The price in Ether for one NFT.
     */
    function getPriceEth() public view returns(uint256) {
        int price = getTokentoUSD();
        return tokenPriceUSD / uint256(price);
    }

    ///
    /// MINT
    ///

    /**
     * @dev Allows anyone to mint NFTs during the public sale phase, provided they send the correct Ether value.
     * @param quantity The number of NFTs to mint.
     * @param to The recipient address of the minted NFTs.
     */
    function mint(uint256 quantity, address to) external payable isContractState(ContractState.PUBLIC) withinMintLimit(quantity) correctValue(getPriceEth() * quantity) {
        require(_numberMinted(to) + quantity < allowancePlusOne, "Exceeds allowance");
        require(quantity < txLimitPlusOne, "Exceeds transaction limit");

        Address.sendValue(payable(treasury), msg.value);

        _mint(to, quantity, "", false);
    }

    // /**
    //  * @dev Allows addresses on the allowlist to mint NFTs during the private sale phase, using a valid Merkle proof.
    //  * @param quantity The number of NFTs to mint.
    //  * @param to The recipient address of the minted NFTs.
    //  * @param proof The Merkle proof validating the address's inclusion in the allowlist.
    //  * @param leaf The Merkle leaf corresponding to the minter's address.
    //  */
    // function mintPrivate(uint256 quantity, address to, bytes32[] calldata proof, bytes32 leaf) external payable isContractState(ContractState.PRIVATE) withinMintLimit(quantity) correctValue(getPriceEth() * quantity) {
    //     require(_numberMinted(to) + quantity < allowancePlusOne, "Exceeds allowance");
    //     require(quantity < txLimitPlusOne, "Exceeds transaction limit");
    //     require(verify(merkleRoot, leaf, proof), "Not a valid proof");

    //     _safeMint(to, quantity);
    // }

    /**
     * @dev Allows public minting using USDC for payment.
     * @param quantity The number of NFTs to mint.
     * @param to The recipient address of the minted NFTs.
     */
    function mintUSDC(uint256 quantity, address to) external isContractState(ContractState.PUBLIC) withinMintLimit(quantity) {
        require(_numberMinted(to) + quantity < allowancePlusOne, "Exceeds allowance");
        require(quantity < txLimitPlusOne, "Exceeds transaction limit");

        IERC20Metadata(USDC).transferFrom(msg.sender, treasury, (tokenPriceUSD * 10 ** IERC20Metadata(USDC).decimals() * quantity) / 10 ** 26);
        _safeMint(to, quantity);
    }

    /**
     * @dev Allows public minting using VEXT for payment.
     * @param quantity The number of NFTs to mint.
     * @param to The recipient address of the minted NFTs.
     */
    function mintVEXT(uint256 quantity, address to) external isContractState(ContractState.PUBLIC) withinMintLimit(quantity) {
        require(_numberMinted(to) + quantity < allowancePlusOne, "Exceeds allowance");
        require(quantity < txLimitPlusOne, "Exceeds transaction limit");

        IERC20Metadata(VEXT).transferFrom(msg.sender, treasury, tokenPriceVEXT * quantity);
        _safeMint(to, quantity);
    }

    // /**
    //  * @dev Allows allowlisted addresses to mint NFTs using an approved ERC20 token during the private sale phase.
    //  * @param quantity The number of NFTs to mint.
    //  * @param to The recipient address of the minted NFTs.
    //  * @param contractAddress The ERC20 token contract address used for payment.
    //  * @param proof The Merkle proof validating the address's inclusion in the allowlist.
    //  * @param leaf The Merkle leaf corresponding to the minter's address.
    //  */
    // function mintPrivateERC20(uint256 quantity, address to, address contractAddress, bytes32[] calldata proof, bytes32 leaf) external isContractState(ContractState.PRIVATE) withinMintLimit(quantity) {
    //     require(erc20Payments[contractAddress].approved, "ERC20 Token not approved!");
    //     require(_numberMinted(to) + quantity < allowancePlusOne, "Exceeds allowance");
    //     require(quantity < txLimitPlusOne, "Exceeds transaction limit");
    //     require(verify(merkleRoot, leaf, proof), "Not a valid proof");

    //     IERC20Metadata(contractAddress).transferFrom(msg.sender, address(this), erc20Payments[contractAddress].tokenPrice * quantity);
    //     _safeMint(to, quantity);
    // }

    /**
     * @dev Allows the contract owner to mint NFTs to a specified address, bypassing payment and state checks.
     * @param to The recipient address of the minted NFTs.
     * @param quantity The number of NFTs to mint.
     */
    function mintAdmin(address to, uint256 quantity) external onlyAdmin(msg.sender) withinMintLimit(quantity) {
        _safeMint(to, quantity);
    }

    /**
     * @dev Burns a specific tokenId. Can only be called by the token owner or an approved operator.
     * @param tokenId The unique identifier for the NFT being burned.
     */
    function burnAdmin(uint256 tokenId) external onlyAdmin(msg.sender) {
        _burn(tokenId);
    }

    ///
    /// ADMIN
    ///

    /**
     * @dev Updates the contract's operational state. Only the contract owner can call this function.
     * @param contractState_ The new operational state of the contract.
     */
    function setContractState(uint256 contractState_) external onlyOwner {        
        contractState = ContractState(contractState_);
    }

    /**
     * @notice Set the admin status for a specific address.
     * @dev Only the owner can call this function.
     * @param _admin The address for which to set the admin status.
     * @param _status The admin status to set (true for admin, false for non-admin).
     */
    function setAdmin(address _admin, bool _status) external onlyOwner {
        admins[_admin] = _status;
    }

    /**
    * Allows admin to set treasury wallet
    */
    function setTreasury(address walletAddress) external onlyOwner {
        require(walletAddress != address(0), "Invalid Address!");
        treasury = walletAddress;
    }

     /**
    * Allows admin to set USDC
    * @notice usdRate is calculated by taking the USD price and multiplying by how many tokens of this ERC20 should equal 1$. 
    */
    function setUSDC(address contractAddress) external onlyOwner {
        require(contractAddress != address(0), "Invalid Address!");
        USDC = contractAddress;
    }

    /**
    * Allows admin to set VEXT
    */
    function setVEXT(address contractAddress) external onlyOwner {
        require(contractAddress != address(0), "Invalid Address!");
        VEXT = contractAddress;
    }

    /**
     * @dev Updates the address of the Chainlink price feed. Only the contract owner can call this function.
     * @param newFeed The new Chainlink price feed contract address.
     */
    function setPriceFeed(address newFeed) external onlyOwner {
        priceFeed = AggregatorV3Interface(newFeed);
    }

    /**
     * @dev Updates the price of NFTs in USD. Only the contract owner can call this function.
     * @param tokenPrice_ The new price per NFT in USD, assuming 8 decimal places.
     */
    function setTokenPriceUSD(uint256 tokenPrice_) external onlyOwner {
        tokenPriceUSD = tokenPrice_;
    }

    /**
     * @dev Updates the maximum number of NFTs available for sale. Only the contract owner can call this function.
     * @param maxSale The new maximum number of NFTs that can be minted.
     */
    function setMaxSale(uint256 maxSale) external onlyOwner {
        uint256 maxSalePlusOne_ = maxSale + 1;
        require(maxSalePlusOne_ > _totalMinted(), "Must be above current supply!");
        maxSalePlusOne = maxSalePlusOne_;
    }

    /**
     * @dev Updates the transaction limit for minting NFTs in public sales. Only the contract owner can call this function.
     * @param txLimit The new transaction limit.
     */
    function setTxLimit(uint256 txLimit) external onlyOwner {
        uint256 txLimitPlusOne_ = txLimit + 1;
        txLimitPlusOne = txLimitPlusOne_;
    }

    /**
     * @dev Updates the minting allowance for each address. Only the contract owner can call this function.
     * @param newAllowance The new minting allowance for each address.
     */
    function setAllowancePlusOne(uint256 newAllowance) external onlyOwner {
        allowancePlusOne = newAllowance + 1;
    }

    // /**
    //  * @dev Updates the Merkle root for the allowlist. Only the contract owner can call this function.
    //  * @param merkleRoot_ The new Merkle root.
    //  */
    // function setMerkleRoot(bytes32 merkleRoot_) external onlyOwner {
    //     merkleRoot = merkleRoot_;
    // }

    /**
     * @dev Updates the base URI for NFT metadata. Only the contract owner can call this function.
     * @param baseURI_ The new base URI.
     */
    function setBaseURI(string memory baseURI_) external onlyOwner {
        baseURI = baseURI_;
    }

    /**
     * @dev Updates the placeholder URI for unrevealed NFTs. Only the contract owner can call this function.
     * @param placeholderURI_ The new placeholder URI.
     */
    function setPlaceholderURI(string memory placeholderURI_) external onlyOwner {
        placeholderURI = placeholderURI_;
    }

    /**
     * @dev Updates the default royalty information. Only the contract owner can call this function.
     * @param receiver The address to receive royalty payments.
     * @param feeNumerator The numerator for calculating royalty payments.
     */
    function setDefaultRoyalty(address receiver, uint96 feeNumerator) external onlyOwner {
        require(receiver != address(0), "Invalid Address!");
        _setRoyalties(receiver, feeNumerator);
    }

    ///
    /// VIEWS
    ///

    /**
     * @dev Returns the timestamp of when a token was last transferred, useful for determining how long a token has been held.
     * @param tokenId The unique identifier for the NFT.
     * @return The timestamp of the last transfer.
     */
    function holdingSince(uint256 tokenId) public view returns (uint256) {
        return _ownershipOf(tokenId).startTimestamp;
    }

    /**
     * @dev Provides various pieces of information about the sale for a given address.
     * @param addr The address to query information for.
     * @return An array containing the contract state, max sale limit, total minted, token price, number minted by the address, and the minting allowance.
     */
    function saleInfo(address addr) public view returns (uint256[7] memory) {
        return [
            uint256(contractState),
            maxSalePlusOne - 1,
            _totalMinted(),
            tokenPriceUSD,
            tokenPriceVEXT,
            _numberMinted(addr),
            allowancePlusOne - 1
        ];
    }

    /**
     * @dev Returns the token URI for a given token ID, providing the metadata link.
     * @param tokenId The unique identifier for the NFT.
     * @return A string representing the metadata URI for the given token ID.
     */
    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(uint16(tokenId)), "URI query for nonexistent token");

        return bytes(baseURI).length > 0 ? string(abi.encodePacked(baseURI, tokenId.toString())) : placeholderURI;
    }

    // /**
    //  * @dev Verifies if a given leaf is part of the Merkle tree using the provided Merkle proof.
    //  * @param root The Merkle root of the allowlist.
    //  * @param leaf The leaf node representing the allowlisted address.
    //  * @param proof The Merkle proof used to validate the leaf's inclusion in the Merkle tree.
    //  * @return A boolean indicating whether the leaf is part of the Merkle tree.
    //  */
    // function verify(bytes32 root, bytes32 leaf, bytes32[] memory proof) public pure returns (bool) {
    //     return MerkleProof.verify(proof, root, leaf);
    // }

    /**
     * @dev Overrides the _startTokenId function from ERC721A to change the starting token ID from 0 to 1, aligning with common NFT standards.
     * @return The starting token ID, which is set to 1.
     */
    function _startTokenId() internal pure override returns (uint256) {
        return 1;
    }

    ///
    /// MISC
    ///

    /**
     * @dev Allows the contract owner to withdraw Ether from the contract to a specified address.
     * @param account The address to withdraw Ether to.
     * @param amount The amount of Ether to withdraw.
     */
    function ownerWithdraw(address payable account, uint256 amount) external onlyOwner {
        require(account != address(0), "Withdrawal to the zero address.");
        require(address(this).balance >= amount, "Insufficient balance for withdrawal.");
        Address.sendValue(account, amount);
    }

    /**
     * @dev Allows the contract owner to withdraw ERC20 tokens from the contract to a specified address.
     * @param tokenAddress The address of the ERC20 token to withdraw.
     * @param account The address to withdraw the tokens to.
     * @param amount The amount of tokens to withdraw.
     */
    function ownerWithdrawERC20(address tokenAddress, address account, uint256 amount) external onlyOwner {
        require(account != address(0) && tokenAddress != address(0), "Invalid address.");
        IERC20 token = IERC20(tokenAddress);
        require(token.balanceOf(address(this)) >= amount, "Insufficient token balance.");
        token.transfer(account, amount);
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     * @param interfaceId The interface identifier, as specified in ERC-165.
     * @return True if the contract implements the interface with the specified ID, false otherwise.
     */
    function supportsInterface(bytes4 interfaceId) public view override(IERC165, ERC721A, ERC2981) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}