    event ReflectNFTAddressUpdated(address indexed newAddress);
    event VaultAddressUpdated(address indexed newAddress);
    event MaxTransactionAmountUpdated(uint256 newAmount);
    event SellFeeUpdated(uint256 newFee);
    event DevPercentageUpdated(uint256 newPercentage);
    event ReflectPercentageUpdated(uint256 newPercentage);
    event VaultPercentageUpdated(uint256 newPercentage);
    event BuyFeePercentageUpdated(uint256 newBuyFeePercentage);
    event SellFeePercentageUpdated(uint256 newSellFeePercentage);
    event ExtraSellFeeWithin24HoursUpdated(uint256 newExtraSellFee);
    event ExtraSellFeeWithin30MinutesUpdated(uint256 newExtraSellFee);

