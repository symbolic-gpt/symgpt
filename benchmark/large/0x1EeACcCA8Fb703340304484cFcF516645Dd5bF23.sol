    uint256 public sellFee = 300;                               // 3% sell fee
    uint256 public extraSellFee = 1000;                         // Extra 10% sell fee for sales within 24 hours
    uint256 public extraExtraSellFee = 1000;                    // Extra 10% sell fee for sales within 30 minutes
    uint256 public devPercentage = 1;                           // 1%
    uint256 public reflectPercentage = 1;                       // 1%
    uint256 public vaultPercentage = 1;                         // 1%
    uint256 public buyFeePercentage = 0;                        // Initial buy fee percentage
    uint256 public sellFeePercentage = 0;                       // Initial sell fee percentage
    uint256 public extraSellFeeWithin24Hours = 1000;            // 10%
    uint256 public extraSellFeeWithin30Minutes = 1000;          // 10%

    address public devWallet;
    address public reflectNFTAddress;
    address public vaultAddress;
    address public preSaleContractAddress;

    IUniswapV2Router02 public uniswapV2Router;

    mapping(address => uint256) private lastSellTime;
    mapping(address => bool) private isExcludedFromFee;
    mapping(address => bool) private isExcludedFromMaxTransaction;
    mapping(address => uint256) private lastBuyTime;

