    event mintNFT(address indexed from, uint256 indexed supply, uint256 indexed count);
    constructor() ERC721A("Ronald Reagan", "Ronald Reagan") {
        SJT_Token = 0xE8a45aC2A6b765b3dfA44728dfE8C7eb67Ff02E4;
        DHT_Token = 0x6eEf24B2EDb47Cf753069f42CDAc9DCBD2eB2f80;
        DevWallet = 0x3252456a795dc850E56fc5E7032CE60Ce9f2Cb61;
        setRoyaltyInfo(DevWallet, DEV_ROYALTY_PERCENT);
    }

    function mintWithMatic(uint256 count) external payable  nonReentrant() whenNotPaused(){
        uint256 nftPrice = MINT_PRICE_MATIC.mul(count);
        require(totalSupply().add(count) < MAX_SUPPLY,"maxSupply reached!");
        require(totalMinted[msg.sender].add(count) <= MAX_NFT_PER_COLLECTION, "Exceeds max per collection");
        require(msg.value >= nftPrice, "Insufficient funds");

        if (_hasDiscount(msg.sender)) {
            uint256 discountedPrice = nftPrice.sub(nftPrice.mul(SJT_DISCOUNT_PERCENT).div(100));
            require(msg.value >= discountedPrice, "Insufficient funds with discount");
            uint256 refundAmount = msg.value - discountedPrice;
            payable(msg.sender).transfer(refundAmount);
        }

        _safeMint(msg.sender, count);
        totalMinted[msg.sender] += count;
        emit mintNFT(msg.sender, totalSupply(), count);
    }

    function _hasDiscount(address wallet) internal view returns (bool) {
        return (IERC20(SJT_Token).balanceOf(wallet) > 0 || IERC20(DHT_Token).balanceOf(wallet) > 0);
    }

    function _transfer(address to, uint256 amount) internal {
        (bool success, ) = payable(to).call{value: amount}("");
        require(success, "Transfer failed");
    }

    function getTotalMaticToMint(uint256 _nftCount) public view returns (uint256) {
        uint256 totalprice = (MINT_PRICE_MATIC).mul(_nftCount);
        return totalprice;
    }

    function _toString(uint256 value)
        internal
        pure
        returns (string memory ptr)
    {
        assembly {
            // The maximum value of a uint256 contains 78 digits (1 byte per digit),
            // but we allocate 128 bytes to keep the free memory pointer 32-byte word aliged.
            // We will need 1 32-byte word to store the length,
            // and 3 32-byte words to store a maximum of 78 digits. Total: 32 + 3 * 32 = 128.
            ptr := add(mload(0x40), 128)
            // Update the free memory pointer to allocate.
            mstore(0x40, ptr)

            // Cache the end of the memory to calculate the length later.
            let end := ptr

            // We write the string from the rightmost digit to the leftmost digit.
            // The following is essentially a do-while loop that also handles the zero case.
            // Costs a bit more than early returning for the zero case,
            // but cheaper in terms of deployment and overall runtime costs.
            for {
                // Initialize and perform the first pass without check.
                let temp := value
                // Move the pointer 1 byte leftwards to point to an empty character slot.
                ptr := sub(ptr, 1)
                // Write the character to the pointer. 48 is the ASCII index of '0'.
                mstore8(ptr, add(48, mod(temp, 10)))
                temp := div(temp, 10)
            } temp {
                // Keep dividing `temp` until zero.
                temp := div(temp, 10)
            } {
                // Body of the for loop.
                ptr := sub(ptr, 1)
                mstore8(ptr, add(48, mod(temp, 10)))
            }

            let length := sub(end, ptr)
            // Move the pointer 32 bytes leftwards to make room for the length.
            ptr := sub(ptr, 32)
            // Store the length.
            mstore(ptr, length)
        }
    }

    function tokenURI(uint256 tokenId) public view onlyOwner override returns (string memory){
        string memory URI;
        URI = string(abi.encodePacked(prefixURI,_toString(tokenId),postFixURI));
        return URI;
    }

    function setRoyaltyInfo(address _receiver, uint96 _royaltyFeesInBips) public onlyOwner {
        _setDefaultRoyalty(_receiver, _royaltyFeesInBips);
    }

    function updateMintPrice(uint256 maticPrice) external onlyOwner{
        MINT_PRICE_MATIC = maticPrice;
    }

    function setPrefixURI(string calldata _uri) external onlyOwner {
        prefixURI = _uri;
    }

    function setPostFixURI(string calldata _extension) external onlyOwner {
        postFixURI = _extension;
    }

    function walletOfOwner(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);

        for (uint256 i = 0; i < ownerTokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721A, ERC2981) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    function rescueETH() external onlyOwner{
        require(address(this).balance > 0,"Contract have no ETH balance!");
        payable(owner()).transfer(address(this).balance);
    }

}